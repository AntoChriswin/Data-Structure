#include<stdio.h>
int main()
{
    int n1,n2,i,c,j=0;
    printf("enter size of two array : ");
    scanf("%d %d",&n1,&n2);
    int t = n1+n2;
    int ab[t];
    int a[n1];
    int b[n2];
    for(i=0;i<n1;i++)
    {
        printf("enter %d element :",i);
        scanf("%d",&a[i]);
    }
    for(i=0;i<n2;i++)
    {
        printf("enter %d element :",i);
        scanf("%d",&b[i]);
    }
    for(i=0;i<n1;i++)
    {
        ab[i] = a[i];
        //c=i;
    }
    for(i=c=n1;i<t;i++)
    {
        ab[i] = b[j];
        j++;
    }
    for(i=0;i<t;i++)
    {
        printf("%d,",ab[i]);
    }
    return 0;

}
